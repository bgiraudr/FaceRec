<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/mainFragment">
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.tituya.facerec.ui.intent.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_manageFacesActivity"
            app:destination="@id/manageFacesActivity" />
    </fragment>
    <activity
        android:id="@+id/manageFacesActivity"
        android:name="com.tituya.facerec.ManageFacesActivity"
        android:label="activity_manage_faces"
        tools:layout="@layout/activity_manage_faces" />
</navigation>